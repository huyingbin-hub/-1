page {
  height: 100%;
}
.jl .no-data {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin-top: 110px;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.jl .no-data image {
  width: 99px;
  height: 106px;
}
.jl .no-data text {
  font-size: 12px;
  color: #999999;
  margin-top: 10px;
}
.jl scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.jl .scroll-view_H {
  white-space: nowrap;
  overflow: hidden;
}
.jl .scroll-view-item {
  max-width: 150rpx;
  overflow: hidden;
  padding: 4px 10px;
  height: 45rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: inline-block;
  /*text-overflow: ellipsis;
		  display: -webkit-box;
		  -webkit-line-clamp: 1;
		  -webkit-box-orient: vertical; */
  border-bottom: 1px solid #F8F8F8;
}
.jl .scroll-view-item .line {
  height: 1px;
}
.jl .scroll-view-item.active {
  color: #FF6900;
}
.jl .scroll-view-item.active .line {
  background-color: #FF6900;
  margin: 0 6px;
  margin-top: 7px;
}
.jl .scroll-view-item:last-child {
  margin-right: 0;
}
.jl .scroll-view-item image {
  width: 100%;
  height: 120rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.jl .title_level1 {
  font-size: 12px;
}
.jl .title_level2 {
  font-size: 24rpx;
  color: #C8C7CC;
}
.jl .list-all {
  padding: 10px 16px;
}
.jl .list-all .list-time {
  font-size: 12px;
  color: #333333;
  margin: 10px 0;
}
.jl .list-all .list {
  font-size: 12px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  line-height: 35px;
  color: #999999;
}
.jl .list-all .list .left {
  width: 220px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

