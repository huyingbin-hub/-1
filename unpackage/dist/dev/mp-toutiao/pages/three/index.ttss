.three {
  color: #333333;
  padding: 0 16px;
}
.three .my-list {
  line-height: 30px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.three .my-list .details text {
  vertical-align: middle;
  margin-left: 5px;
  font-size: 12px;
}
.three .sign-out {
  margin: 20px 0;
}
.three .sign-out button {
  width: 100%;
  font-size: 14px;
  height: 35px;
  line-height: 35px;
}
.three .no-login .nav {
  height: 60px;
  font-size: 14px;
  line-height: 86px;
  text-align: center;
}
.three .no-login .login {
  margin-top: 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 80px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.three .no-login .login .login-avatar image {
  -webkit-border-radius: 50%;
          border-radius: 50%;
}
.three .no-login .login .login-now {
  margin-left: 20px;
}
.three .no-login .login .login-now view {
  font-size: 14px;
}
.three .no-login .login .login-now text {
  color: #999999;
  font-size: 12px;
}
.three .login-after .nav-after {
  color: #FFFFFF;
  margin: -16px;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  height: 173px;
  text-align: center;
}
.three .login-after .user-info {
  position: relative;
  top: -30px;
  width: 100%;
  height: 133px;
  background: #FFFFFF;
  -webkit-box-shadow: 0px 4px 8px 0px rgba(0, 0, 0, 0.15);
          box-shadow: 0px 4px 8px 0px rgba(0, 0, 0, 0.15);
  -webkit-border-radius: 10px;
          border-radius: 10px;
}
.three .login-after .user-info .user-img {
  position: absolute;
  width: 100%;
  /* padding: 0 132px; */
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  top: -30px;
}
.three .login-after .user-info .user-img image {
  -webkit-border-radius: 50%;
          border-radius: 50%;
  display: block;
  margin: 0 4px;
}
.three .login-after .user-info .user-img text {
  width: 100%;
  text-align: center;
  font-size: 12px;
  margin-top: 5px;
}
.three .login-after .user-info .user-study {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 10px;
}
.three .login-after .user-info .user-study .dat {
  font-size: 16px !important;
  line-height: 27px;
}
.three .login-after .user-info .user-study .time {
  position: absolute;
  bottom: 15px;
  left: 20px;
}
.three .login-after .user-info .user-study .time view {
  text-align: center;
  font-size: 12px;
}
.three .login-after .user-info .user-study .days {
  position: absolute;
  bottom: 15px;
  right: 20px;
}
.three .login-after .user-info .user-study .days view {
  text-align: center;
  font-size: 12px;
}

