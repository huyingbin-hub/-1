  width: 90%;
  background-color: #FF6900;
  -webkit-border-radius: 22px;
          border-radius: 22px;
}
.content .xm-chose .all {
  overflow: hidden;
  height: 100%;
  font-size: 14px;
}
.content .xm-chose .all .left {
  float: left;
  width: 120px;
  padding: 5px 20px 5px 0;
  margin-left: 5px;
  height: 100%;
  position: relative;
  border-right: 1px solid #EEEEEE;
}
.content .xm-chose .all .left view {
  padding-left: 20px !important;
  font-weight: bold;
  margin: 10px 0;
  margin-bottom: 29px !important;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.content .xm-chose .all .left view.active {
  color: #FF6900;
  border-left: 2px solid #FF6900;
}
.content .xm-chose .all .right {
  overflow: hidden;
  margin-left: 20px;
  padding-left: 20px;
  margin: 10px;
}
.content .xm-chose .all .right view {
  margin: 10px 0;
  margin-bottom: 16px !important;
}
.content .xm-chose .all .right view.active {
  color: #FF6900;
}
.content .chose-content {
