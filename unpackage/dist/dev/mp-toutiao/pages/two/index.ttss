scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.scroll-view_H {
  white-space: nowrap;
  overflow: hidden;
}
.scroll-view-item {
  max-width: 150rpx;
  overflow: hidden;
  padding: 4px 10px;
  height: 45rpx;
  display: inline-block;
  border-bottom: 1px solid #F8F8F8;
}
.scroll-view-item .line {
  height: 1px;
}
.scroll-view-item.active {
  color: #FF6900;
}
.scroll-view-item.active .line {
  background-color: #FF6900;
  margin: 0 6px;
  margin-top: 7px;
}
.scroll-view-item:last-child {
  margin-right: 0;
}
.scroll-view-item image {
  width: 100%;
  height: 120rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.title_level1 {
  font-size: 12px;
}
.title_level2 {
  font-size: 24rpx;
  color: #C8C7CC;
}
.content {
  color: #333333;
}
.content .model-km .km-content {
  padding-bottom: 10px;
}
.content .model-km .km-content .title {
  font-size: 14px;
  margin-left: 16px;
  padding-top: 10px;
}
.content .model-km .km-content .km-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 10px 18px;
  margin: 0 -10px;
}
.content .model-km .km-content .km-list .km-text {
  font-size: 12px;
  width: 20%;
  line-height: 26px;
  margin: 5px 6px;
  text-align: center;
  display: inline-block;
  border: 1px solid #EEEEEE;
}
.content .model-km .km-content .km-list .km-text.activekm {
  color: #FFFFFF;
  background-color: #FF6900;
}
.content .model-km .km-content .config-km {
  margin: 10px 18px;
